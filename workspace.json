{
  "version": 2,
  "projects": {
    "expo-test-app": {
      "root": "apps/expo-test-app",
      "sourceRoot": "apps/expo-test-app/src",
      "projectType": "application",
      "generators": {},
      "targets": {
        "bundle": {
          "executor": "nx-react-native-expo:bundle",
          "options": {}
        },
        "start": {
          "executor": "nx-react-native-expo:start",
          "options": {}
        },
        "run-ios": {
          "executor": "nx-react-native-expo:run-ios",
          "options": {}
        },
        "run-android": {
          "executor": "nx-react-native-expo:run-android",
          "options": {}
        },
        "run-web": {
          "executor": "nx-react-native-expo:run-web",
          "options": {}
        },
        "publish": {
          "executor": "nx-react-native-expo:publish",
          "options": {}
        },
        "prestart": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["sbn-get-stories"],
            "cwd": "apps/expo-test-app"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/expo-test-app/**/*.{js,ts,tsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/expo-test-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nx-expo-storybook": {
      "root": "libs/nx-expo-storybook",
      "sourceRoot": "libs/nx-expo-storybook/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/nx-expo-storybook/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/nx-expo-storybook"],
          "options": {
            "jestConfig": "libs/nx-expo-storybook/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/nx-expo-storybook",
            "tsConfig": "libs/nx-expo-storybook/tsconfig.lib.json",
            "packageJson": "libs/nx-expo-storybook/package.json",
            "main": "libs/nx-expo-storybook/src/index.ts",
            "assets": [
              "libs/nx-expo-storybook/*.md",
              {
                "input": "./libs/nx-expo-storybook/src",
                "glob": "**/!(*.ts)",
                "output": "./src"
              },
              {
                "input": "./libs/nx-expo-storybook/src",
                "glob": "**/*.d.ts",
                "output": "./src"
              },
              {
                "input": "./libs/nx-expo-storybook",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./libs/nx-expo-storybook",
                "glob": "executors.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "nx-expo-storybook-e2e": {
      "root": "apps/nx-expo-storybook-e2e",
      "projectType": "application",
      "sourceRoot": "apps/nx-expo-storybook-e2e/src",
      "targets": {
        "e2e": {
          "executor": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "nx-expo-storybook:build",
            "jestConfig": "apps/nx-expo-storybook-e2e/jest.config.js"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "defaultProject": "expo-test-app"
}
